					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.expect(pm.response.code).to.be.equal(200);",
									"});",
									"",
									"let createdId = pm.response.text();",
									"let postBody = JSON.parse(pm.request.body.raw);",
									"",
									"pm.sendRequest({",
									"    url: pm.variables.replaceIn(\"{{base}}/api/film/\" + createdId),",
									"    method: \"GET\",",
									"    header: {",
									"        \"Content-Type\": \"application/json\"",
									"    }",
									"}, function (err, res) {",
									"    pm.test(\"GET created film returns 200\", function () {",
									"        pm.expect(res.code).to.be.equal(200);",
									"    });",
									"    pm.test(\"GET created film returns correct data\", function () {",
									"        let getData = res.json();",
									"        pm.expect(getData).to.have.property(\"title\", postBody.title);",
									"        pm.expect(getData).to.have.property(\"description\", postBody.description);",
									"    });",
									"});",
									""
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
